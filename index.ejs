	//form validation
	$( document ).ready(function(){

		//close navbar if click outside
		$(document).click(function (e) {  
			if($('.collapse').hasClass('show') && !$('nav').is(e.target) && $('nav').has(e.target).length === 0){
				$('.navbar-toggler').click()
			}
		})
		
			$("#name_error_message").css('display', 'none');
        	$("#message_error_message").css('display', 'none');
			$("#email_error_message").css('display', 'none');
				
			var formsubmit;	
			
		
			//check the name is vaild 
			function check_name() {	
						
				var fname = $("#name").val();
				if (fname !== '') {
				$("#name_error_message").css('display', 'none');	
				$("#name").css("border","");		 
				formsubmit1 = false;
				} else {
				$("#name_error_message").html("Enter Your name");			   
				$("#name_error_message").css('display', 'block');		
				$("#name").css("border","2px solid  #FCE205");
				formsubmit1 = true;
				return false;
				}			
			
		 	}

			 //check the message is vaild 
			function check_message() {
					
				var mess = $("#messagefield").val();
				if (mess !== '') {
				$("#message_error_message").css('display', 'none');
				$("#messagefield").css("border","");				   		 
				formsubmit2 =false;
				}
				else {
				$("#message_error_message").html("Enter your message");
				$("#message_error_message").css('display', 'block');
				$("#messagefield").css("border","2px solid  #FCE205");
				formsubmit2 = true;
				return false;
				}		
							
		}
		
        //check the email vaild
		function check_email() {
            var pattern = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
            var email = $("#email").val();
            if (pattern.test(email) && email !== '') {
               $("#email_error_message").css('display', 'none');
			   $("#email").css("border","");	
			   formsubmit3 = false;
            } else {
               $("#email_error_message").html("Enter the vaild email");
               $("#email_error_message").css('display', 'block');
			   $("#email").css("border","2px solid  #FCE205");
			   
			   formsubmit3 = true;
			   return false;
			}		
					
         }  
	
		

		 $( "#message-details" ).submit(function( event ) {	
			event.preventDefault();
			
			//$('#btnSubmit').attr("disabled", false);
			check_message();	 
			check_name();			
			check_email();
		    
			if (formsubmit1==true || formsubmit2==true  || formsubmit3==true )
			{				
				return false								
			}
			else
			{	
			 
			   //do your own request an handle the results
			   var remote = $.ajax({
			   url: '/message',
			   type: 'post',
			   async: false,
			   dataType: 'application/json',
			   data: $("#message-details").serialize(),
			   // success: function (data) {
			   // 	alert(data)
			   // }
		   }).responseText;
		   if (remote == 'OK') {
			   $("#confirmation").css("display", "block");
			   document.getElementById('name').value = ''
			   document.getElementById('email').value = ''
			   document.getElementById('messagefield').value = ''
		   }
		   else {
			   alert('Could not send your message, please try again')
		   }
		   
		}//else form validation
	   })//form

   }) //Document.ready
	
